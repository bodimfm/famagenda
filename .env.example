# FAMAGENDA - Environment Variables Example
# Copy this file to .env and fill in your API keys

# Google Gemini API (for AI family summaries)
EXPO_PUBLIC_GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API (for AI features)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# RevenueCat (In-App Purchases)
EXPO_PUBLIC_REVENUECAT_APPLE_KEY=your_revenuecat_apple_key_here
EXPO_PUBLIC_REVENUECAT_GOOGLE_KEY=your_revenuecat_google_key_here
EXPO_PUBLIC_REVENUECAT_TEST_KEY=your_revenuecat_test_key_here

# TMDB API (The Movie Database - for movie features)
EXPO_PUBLIC_TMDB_API_KEY=your_tmdb_api_key_here

# Supabase (Backend - optional)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_KEY=your_supabase_anon_key_here
